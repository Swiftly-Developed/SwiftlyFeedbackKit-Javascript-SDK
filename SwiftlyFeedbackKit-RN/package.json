{
  "name": "@feedbackkit/react-native",
  "version": "1.0.0",
  "description": "FeedbackKit React Native SDK - Collect user feedback in your React Native apps",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "bob build",
    "clean": "rm -rf lib",
    "prepare": "npm run build"
  },
  "keywords": [
    "react-native",
    "feedback",
    "feedbackkit",
    "user-feedback",
    "feature-requests",
    "bug-reports",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Swiftly-Developed/SwiftlyFeedbackKit-RN.git"
  },
  "author": "Swiftly Developed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Swiftly-Developed/SwiftlyFeedbackKit-RN/issues"
  },
  "homepage": "https://github.com/Swiftly-Developed/SwiftlyFeedbackKit-RN#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@feedbackkit/js": "^1.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "@react-native-async-storage/async-storage": ">=1.17.0"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.47",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-native": "^0.73.2",
    "react-native-builder-bob": "^0.23.2",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
